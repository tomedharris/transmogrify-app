{
  "name": "transmogrify",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vitest run && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest"
  },
  "dependencies": {
    "@vueuse/components": "^10.2.1",
    "@vueuse/core": "^10.2.1",
    "jshashes": "^1.0.8",
    "mitt": "^3.0.1",
    "node-device-detector": "^2.0.15",
    "pinia": "^2.1.3",
    "prismjs": "^1.29.0",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.3.4",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-router": "^4.2.2",
    "xml-formatter": "^3.4.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6"
  }
}
